	<subDataset name="salarial" uuid="7b099bfb-2eb5-404b-bb81-852a25605706">
		<queryString language="SQL">
			<![CDATA[SELECT(c.salarioactual-d.montodescuento) as Total, concat(e.nombreempleado,' ',e.apellidoempleado) as Empleado,c.salarioactual,DATE_FORMAT(c.fechainiciocontrato, '%d/%m/%Y') as fechainiciocontrato,DATE_FORMAT(c.fechafincontrato, '%d/%m/%Y') as fechafincontrato,c.sistemapago,c.plazanominal,d.montopago,d.montodescuento,el.nombreinstitucion, CURRENT_DATE from empleado e
 inner join contrato c on e.codigoempleado=c.codigoempleado
 inner join descuento d on e.codigoempleado=d.codigodescuento
 inner join experiencialaboral el on el.codigoexperiencialaboral=e.codigoempleado]]>
