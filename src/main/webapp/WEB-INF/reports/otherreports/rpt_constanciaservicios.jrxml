	<queryString>
		<![CDATA[SELECT concat(e.nombreempleado,' ',e.apellidoempleado) as Empleado ,c.fechainiciocontrato,c.fechafincontrato,c.plazanominal,el.nombreinstitucion,c.salarioactual,c.sistemapago, CURRENT_DATE from empleado e inner join contrato c on e.codigoempleado=c.codigoempleado inner join experiencialaboral el
 on e.codigoempleado=el.codigoexperiencialaboral
 WHERE e.codigoempleado=$P{CODIGO} and
