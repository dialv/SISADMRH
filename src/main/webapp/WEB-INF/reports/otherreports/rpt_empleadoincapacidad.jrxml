	<queryString>
		<![CDATA[SELECT concat(nombreempleado,' ',apellidoempleado)as Empleado, nombreincapacidad, nombrepuesto, dui, DATE_FORMAT(fechadesdeincapacidad, '%d/%m/%Y') AS Desde, DATE_FORMAT(fechahastaincapacidad, '%d/%m/%Y') AS Hasta , isss, tipoincapacidad, formaincapacidad, subsidio, DATE_FORMAT(fechaemision, '%d/%m/%Y') AS F_Emosion  FROM `incapacidad` inner join empleado e on incapacidad.codigoempleado= e.codigoempleado inner join puesto on puesto.codigopuesto=e.codigopuesto
 WHERE e.codigoempleado=$P{USUARIO}  and
fechadesdeincapacidad>=$P{FECHAINICIO} and
	fechahastaincapacidad<=$P{FECHAFIN}]]>
