	<queryString>
		<![CDATA[SELECT count(e.codigoempleado)as totalRegistros, concat(DAY(e.fechanacimientoempleado)," / ", MONTH(e.fechanacimientoempleado)) AS Fecha,
p.nombrepuesto,uf.nombreubicacion,e.nombreempleado,
c.numeroacuerdocomite,c.nombrecomite,
c.fechadesdecomite,c.fechahastacomite from SISADMRH.empleado e 
INNER JOIN SISADMRH.puesto p
 ON e.codigoempleado=p.codigoempleado 
INNER JOIN SISADMRH.ubicacionfisica uf
 on uf.codigoempleado=e.codigoempleado inner join comite c
on e.codigoempleado=c.codigoempleado
where  c.codigocomite=$P{CODIGO} and
  c.fechadesdecomite >= $P{FECHAINICIO}
     and c.fechahastacomite <= $P{FECHAFIN}]]>
	</queryString>
