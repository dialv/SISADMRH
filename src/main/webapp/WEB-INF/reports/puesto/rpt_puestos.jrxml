     puesto.`CODIGOPUESTO` AS puesto_CODIGOPUESTO,
     puesto.`CATEGORIAPUESTO` AS puesto_CATEGORIAPUESTO,
     puesto.`SUELDOBASE` AS puesto_SUELDOBASE,
